{
  "name": "lida-api",
  "version": "1.0.0",
  "description": "This is a REST API utilising the CQRS design pattern being exposed via Google Clouds serverless functions.",
  "main": "index.js",
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=460 index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd135hp/LIDA.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asd135hp/LIDA/issues"
  },
  "homepage": "https://github.com/asd135hp/LIDA#readme",
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/*.test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "d.ts",
      "tsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "@google-cloud/storage": "^6.4.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "fflate": "^0.7.3",
    "firebase": "^9.8.4",
    "firebase-admin": "^10.2.0",
    "jest": "^27.3.1",
    "luxon": "^2.4.0",
    "serve-static": "^1.15.0",
    "tsoa": "^4.1.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/luxon": "^2.3.2",
    "@types/node": "^18.7.13",
    "@types/pg": "^8.6.5",
    "@types/serve-static": "^1.13.10",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
