{
  "version": 2,
  "rewrites": [{
    "source": "/(.*)",
    "destination": "index.js"
  }],
  "buildCommand": "yarn run vercel-build",
  "outputDirectory": "public",
  "installCommand": "yarn install",
  "devCommand": "yarn run start",
  "github": {
    "enabled": false
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "max-age=60, public, must-revalidate"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "destination": "index.js"
    }
  ]
}